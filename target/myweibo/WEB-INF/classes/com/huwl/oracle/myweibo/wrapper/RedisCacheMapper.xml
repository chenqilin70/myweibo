<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.huwl.oracle.myweibo.wrapper.RedisCacheMapper" >
    <cache type="org.mybatis.caches.redis.RedisCache" />
    <select id="getDefaultGroupId" resultType="java.lang.Integer" useCache="true">
        select GROUP_ID FROM USER_GROUP WHERE USER_ID=#{id} and GROUP_NAME='未分组'
    </select>
</mapper>